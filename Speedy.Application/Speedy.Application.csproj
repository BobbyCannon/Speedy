<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
		<AssemblyTitle>Speedy.Application</AssemblyTitle>
		<SignAssembly>true</SignAssembly>
		<DelaySign>false</DelaySign>
		<AssemblyOriginatorKeyFile>Speedy.snk</AssemblyOriginatorKeyFile>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>

	<ItemGroup>
		<None Include="Speedy.snk" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Speedy\Speedy.csproj" />
	</ItemGroup>

	<ItemGroup>
		<InternalsVisibleTo Include="Speedy.Application.Maui" Key="" />
		<InternalsVisibleTo Include="Speedy.Application.Wpf" Key="" />
		<InternalsVisibleTo Include="Speedy.Application.Xamarin" Key="" />
	</ItemGroup>
</Project>